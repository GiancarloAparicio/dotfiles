" Disable double quote in files JSON
autocmd Filetype json
  \ let g:indentLine_setConceal = 0 |
  \ let g:vim_json_syntax_conceal = 0

" Save file automatic when lost focus
autocmd FocusLost * silent! wa
autocmd CursorHold,CursorHoldI * update

" Clear buffer in ultiSnips
autocmd BufLeave * call UltiSnips#LeavingBuffer()

" Actualize las vistas de vimdiff si un archivo se guarda en otra ventana
autocmd BufWritePost * if &diff | diffupdate | endif

augroup Php_format
  " Format on save file PHP and Blade
  autocmd! BufwritePre *.php silent! PrettierPhp

  " Enable syntax for files php
  autocmd! bufread *.php silent! source $VIMRUNTIME/syntax/syntax.vim | AirlineRefresh
augroup END

